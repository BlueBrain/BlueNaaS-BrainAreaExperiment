{
  "name": "sim-launcher-gui",
  "version": "0.1.3",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "clean-deep": "^3.0.2",
    "file-saver": "^2.0.0-rc.4",
    "iview": "^3.1.3",
    "jso": "^4.1.1",
    "localforage": "^1.7.2",
    "lodash": "^4.17.11",
    "pouchdb-browser": "^7.0.0",
    "vis": "^4.21.0",
    "vue": "^2.5.17",
    "vue-infinite-loading": "^2.4.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "@vue/eslint-config-airbnb": "^3.0.5",
    "node-sass": "^4.9.4",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.22.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "rules": {
      "no-debugger": 0,
      "no-console": 0,
      "no-underscore-dangle": 0,
      "import/no-named-as-default-member": 0,
      "max-len": [
        "error",
        {
          "code": 120,
          "ignoreStrings": true
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
